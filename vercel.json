{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "api/manifest.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/logo(.*)",
      "dest": "/logo$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/manifest.json",
      "dest": "/api/manifest"
    },
    {
      "src": "/asset-manifest.json",
      "dest": "/asset-manifest.json",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "https://gideons-tech-suite-api.vercel.app/api/$1"
    },
    {
      "src": "/ws",
      "dest": "https://gideons-tech-suite-api.vercel.app/ws"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate"
      }
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "PUBLIC_URL": "/"
  },
  "build": {
    "env": {
      "NODE_ENV": "production",
      "PUBLIC_URL": "/",
      "REACT_APP_API_URL": "https://gideons-tech-suite-api.vercel.app/api",
      "REACT_APP_WS_URL": "wss://gideons-tech-suite-api.vercel.app/ws"
    }
  }
}