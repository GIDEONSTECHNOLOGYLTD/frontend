{
  "version": 2,
  "builds": [
    { 
      "src": "package.json", 
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/logo(.*)",
      "dest": "/logo$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate"
      }
    },
    {
      "src": "/asset-manifest.json",
      "dest": "/asset-manifest.json",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "https://backend-7ck9fowyc-gideonstechnologyltds-projects.vercel.app/api/$1"
    },
    {
      "src": "/ws",
      "dest": "https://backend-7ck9fowyc-gideonstechnologyltds-projects.vercel.app/ws"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate"
      }
    }
  ],
  "env": {
    "PUBLIC_URL": "/",
    "REACT_APP_API_URL": "https://backend-7ck9fowyc-gideonstechnologyltds-projects.vercel.app/api",
    "REACT_APP_WS_URL": "wss://backend-7ck9fowyc-gideonstechnologyltds-projects.vercel.app/ws"
  },
  "build": {
    "env": {
      "PUBLIC_URL": "/",
      "REACT_APP_API_URL": "https://backend-7ck9fowyc-gideonstechnologyltds-projects.vercel.app/api",
      "REACT_APP_WS_URL": "wss://backend-7ck9fowyc-gideonstechnologyltds-projects.vercel.app/ws"
    }
  }
}